<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Approved AMIs" max_length="40" name="x_terraform_snow_approved_amis" type="collection">
        <element label="AMI ID" max_length="255" name="ami_id" type="string"/>
        <element label="AMI Name" max_length="255" name="ami_name" type="string"/>
        <element label="Creation Date" max_length="255" name="creation_date" type="glide_date"/>
        <element choice="1" label="OS" max_length="255" name="operating_system" type="choice">
            <choice>
                <element label="CentOS" value="CentOS"/>
                <element label="RHEL" value="RHEL"/>
                <element label="Windows" value="Windows"/>
            </choice>
        </element>
        <element label="OS Version" max_length="25" name="os_version" type="string"/>
        <element choice="1" label="region" max_length="25" name="region" type="choice">
            <choice>
                <element label="East1" value="us-east-1"/>
                <element label="East2" value="us-east-2"/>
                <element label="West1" value="us-west-1"/>
                <element label="West2" value="us-west-2"/>
            </choice>
        </element>
        <element choice="2" label="Virtualization Type" max_length="25" name="virtualizatio_type" type="choice">
            <choice>
                <element label="Hardware" value="Hardware"/>
            </choice>
        </element>
    </element>
</database>
